{
  "name": "jamband/yii2-schemadump",
  "description": "Generate the schema from an existing database",
  "keywords": ["yii2", "extension", "command", "migration", "generator", "database"],
  "homepage": "https://github.com/jamband/yii2-schemadump",
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Tomoki Morita",
      "email": "tmsongbooks215@gmail.com",
      "homepage": "https://jamband.github.io/"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "yiisoft/yii2": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "jamband\\schemadump\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\": "tests"
    }
  }
}
